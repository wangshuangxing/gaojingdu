/****************************************/
/**************求除法的高精度**************/
/****************************************/


#include <iostream>
#include <cstring>
#include <cstdio>

using namespace std;
int  d[50],v[50];
int main()
{
     long long a,b,c;
     while( cin>>a>>b)
   {
       c=a/b;
    int cun=0;
    if(c<1)
    {
        d[cun]=0;
        cun++;
    }
    while(c>0)
    {
        d[cun++]=c%10;
        c=c/10;
    }
    long long r=a%b;
    r=r*10;
    int i=0;
    while(i<20)
    {
        int s=r/b;
        v[i]=s;
        r=(r%b)*10;
        i++;
    }
    for(int g=i-1;g>=0;i--,g--)
    {
        if(v[g]>0)break;
    }
    cout<<a<<"/"<<b<<"=";
  for(int j=cun-1;j>=0;j--)
  {
      cout<<d[j];
  }
  cout<<'.';
  if(i==0)cout<<'0';
  for(int j=0;j<i;j++)
    {
        cout<<v[j];
    }
    cout<<endl;
   }

    return 0;
}



